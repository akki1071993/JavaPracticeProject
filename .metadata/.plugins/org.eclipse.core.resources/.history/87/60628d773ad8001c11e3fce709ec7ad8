package akash;

public class helloWorld {

	public static void main(String[] args) {
		 System.out.println("Hello world !");
		//System.out.println("Your first argument is: " + args[0]);

	}

}

class A {
	public static void main(String args[]) {

		for (int i = 0; i < args.length; i++)
			System.out.println(args[i]);

	}
}
