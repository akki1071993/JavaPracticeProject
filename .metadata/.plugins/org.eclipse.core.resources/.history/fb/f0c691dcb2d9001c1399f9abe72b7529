package akash;

public class helloWorld {

	public int a= 10;
	protected int b= 20;
	private int c= 30;
    int d= 0;
	
	public static void main(String[] args) {
		//System.out.println("Hello world !");
		
		helloWorld hw = new helloWorld();
		
		System.out.println("Public: "+ hw.a);
		System.out.println("Protected: "+ hw.b);
		System.out.println("Private: "+ hw.c);
		System.out.println("Default: "+ hw.d);
		
		//In within class all accessible 
	}	
}
