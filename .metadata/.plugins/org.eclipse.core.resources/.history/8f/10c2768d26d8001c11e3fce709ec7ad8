package akash;

public class helloWorld {

	public static void main(String[] args) {
		System.out.println("Hello world !");

		String s = "Akash Mahant";
		String s1 = "Akash";
		int re = s1.indexOf(s);

		if (re == 1) {
			System.out.println("Given string is same!");
		} else {
			System.out.println("Given string is not same");
		}

	}

}
