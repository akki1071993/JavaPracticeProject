package akash;

public class helloWorld {

	public static void main(String[] args) {
		System.out.println("Hello world !");

		int a[]= {5,10,15,20,25};
		
		// for loop
		for(int x= 0; x < a.length; x++) {
			System.out.println(a[x]);
		}
		
		// for-each loop
		for (int x : a) {
			System.out.println(x);
		}

	}

}
