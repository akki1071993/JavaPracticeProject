package akash;

public class helloWorld {

	public static void main(String[] args) {
		System.out.println("Hello world !");		
	    System.out.println(Sum.Add(10,20));			
		}

	}



class Sum {
	static int Add(int x, int y) {
		return x + y;
	}
	
	static float Add (float x, float y) {
		return x + y;
	}
}


